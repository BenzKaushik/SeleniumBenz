	 }
	 
	  @Test(groups = {"functest", "SmokeTest"} )
	  public void testMethod2() {
		  
	  }
	 
	  @Test(groups = {"Regression" })
	  public void testMethod3() {
	  }
	  
	  @Test(groups = {"functest", "Regression" })
	  public void testMethod4() {
		  
	  }



}
