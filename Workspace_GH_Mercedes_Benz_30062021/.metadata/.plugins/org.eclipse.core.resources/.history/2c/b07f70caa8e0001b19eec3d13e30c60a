  	</classes>
   </test>
    
    
    <!-- Regular Expression (*) grouping based on platform -->
   <test name="RegularExpressionTest2">
   	<groups>
    	<run>
     	 <include name="windows.*"></include>
    	</run>
  	</groups>
   	
	<classes>
    	<class name="Group.Example2"/>
  	</classes>
   </test>
   
   
  
   
  
 
